<!-- food section -->
<section class="food_section layout_padding-bottom">
    <div class="container" id="menu">
        <div class="heading_container heading_center">
            <h2>
                Khóa Học của chúng tôi
            </h2>
        </div>
        <ul class="filters_menu">
            <li class="active" data-filter="*">Tất cả</li>
            <li data-filter=".8">Lập trình game</li>
            <li data-filter=".15">Lập Trình Angular</li>
            <li data-filter=".13">Lập Trình Java</li>
            <li data-filter=".14">Lập Trình Oracle</li>
            <li data-filter=".12">Lập Trình PHP</li>
            <div class="search">
                @using (Html.BeginForm("LayTuKhoaTimKiem", "TimKiem"))
                {
                    <input type="text" name="sTuKhoa" placeholder="Tên sản phẩm ...">
                    <input type="submit" value="Tìm kiếm">
                }
            </div>
        </ul>

        <div class="filters-content">
            <div class="row grid">
                @Html.Partial("~/Views/SanPham/SanPhamStyle1Partial.cshtml", lstDTM)

            </div>
        </div>
